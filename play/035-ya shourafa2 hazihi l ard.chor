# Total time: ~49 seconds
# Romantic slow swaying motion back and forth

# Gentle sway right
set_frps 0.02
sync {
    move(1, 0.45)
    move(2, 0.5)
    move(4, 0.42)
}
wait(1)

# Sway left
set_frps 0.02
sync {
    move(1, 0.25)
    move(2, 0.15)
    move(4, 0.25)
}
wait(1.5)


# Reset to zero for next scene
set_frps 0.1
sync {
    move(1, 0)
    move(2, 0)
    move(4, 0)
}
